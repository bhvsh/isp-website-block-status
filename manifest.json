{
    "name": "Is it blocked?",
    "version": "1.0.0",
    "description": "Checks whether the domain of the website is blocked or not",
    "manifest_version": 3,
    "author": "bhvsh",
    "action":{
        "default_popup": "index.html",
        "default_title": "Is the site blocked?"
    },
    "icons": {
        "16": "icons/icon16.png",
        "32": "icons/icon32.png",
        "48": "icons/icon48.png",
        "128": "icons/icon128.png"
      },    
    "permissions": [
    "tabs"
    ]
}